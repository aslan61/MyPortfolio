@model List<MyPortfolio.DAL.Entities.Feature>
<!-- Feature(Özellikler)-->
@foreach (var item in Model)
{
    <section class="resume-section" id="interests">
        <div class="resume-section-content">
            <h2 class="mb-5">@item.Title</h2>
            <p>@item.Description</p>
            
        </div>
    </section>
}
